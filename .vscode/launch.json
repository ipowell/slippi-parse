  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "script.ts",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "runtimeArgs": ["--preserve-symlinks"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Percentage Finder",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/stockLead/percentageFinder.ts",
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "runtimeArgs": ["--preserve-symlinks"]
    }
  ]
}
